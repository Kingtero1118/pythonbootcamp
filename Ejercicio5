def bisiesto(year):
    if year % 4 == 0 and year % 100 != 0:
        print("Es un a単o bisiesto")
    elif year % 100 == 0 and year % 400 ==0:
        print("Es un a単o bisiesto")
    elif year % 100 == 0 and year % 400 !=0:
        print("No es un a単o bisiesto")
    else:
        print("No es un a単o bisiesto")

bisiesto(2021)
